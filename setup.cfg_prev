[metadata]
name = basic_pid
version = attr: basic_pid.__version__
author = Mike Knerr
#author_email = author@site.domain
home_page = https://github.com/mkner/basic-pid
readme = README.md
description = basic pid controller for discretized time
#long_description = file: README.rst, CHANGELOG.rst, LICENSE.rst
long_description = file: README.md
license = MIT
classifiers = 
   Programming Language :: Python :: 3
   License :: OSI Approved :: MIT License
   Operating System :: OS Independent
   
keywords = python packages, pypi   

[options]
include_package_data = True
packages = find:
python_requires = >=3.6


